{
   "base": "vs-dark",
   "inherit": true,
   "rules": [
     { "token": "", "foreground": "#6688cc", "background": "#000c18" },
     { "token": "meta.embedded", "foreground": "#6688cc" },
     { "token": "comment", "foreground": "#384887" },
     { "token": "string", "foreground": "#22aa44" },
     { "token": "constant.numeric", "foreground": "#f280d0" },
     { "token": "constant.language", "foreground": "#f280d0" },
     { "token": "constant.character", "foreground": "#f280d0" },
     { "token": "constant.other", "foreground": "#f280d0" },
     { "token": "keyword", "foreground": "#225588" },
     { "token": "storage", "foreground": "#225588" },
     { "token": "storage.type", "foreground": "#9966b8", "fontStyle": "italic" },
     { "token": "entity.name.class", "foreground": "#ffeebb", "fontStyle": "underline" },
     { "token": "entity.name.type", "foreground": "#ffeebb", "fontStyle": "underline" },
     { "token": "entity.other.inherited-class", "foreground": "#ddbb88", "fontStyle": "italic underline" },
     { "token": "entity.name.function", "foreground": "#ddbb88" },
     { "token": "variable.parameter", "foreground": "#2277ff", "fontStyle": "italic" },
     { "token": "entity.name.tag", "foreground": "#225588" },
     { "token": "entity.other.attribute-name", "foreground": "#ddbb88" },
     { "token": "support.function", "foreground": "#9966b8" },
     { "token": "support.constant", "foreground": "#9966b8" },
     { "token": "support.type", "foreground": "#9966b8", "fontStyle": "italic" },
     { "token": "support.class", "foreground": "#9966b8", "fontStyle": "italic" },
     { "token": "invalid", "foreground": "#A22D44" },
     { "token": "markup.heading", "foreground": "#6688cc", "fontStyle": "bold" },
     { "token": "tag", "foreground": "#225588" },
     { "token": "attribute.name", "foreground": "#ddbb88" },
     { "token": "property", "foreground": "#9966b8" },
     { "token": "value", "foreground": "#22aa44" }
   ],
   "colors": {
     "editor.background": "#000c18",
     "editor.foreground": "#6688cc",
     "editorCursor.foreground": "#ddbb88",
     "editorWhitespace.foreground": "#103050",
     "editor.lineHighlightBackground": "#082050",
     "editor.selectionBackground": "#770811",
     "editorIndentGuide.background": "#002952",
     "editorIndentGuide.activeBackground": "#204972",
     "editorLineNumber.foreground": "#406385",
     "editorLineNumber.activeForeground": "#80a2c2",
     "editorHoverWidget.background": "#000c38",
     "editorHoverWidget.border": "#004c18",
     "editor.findMatchHighlightBackground": "#eeeeee44",
     "editorWidget.background": "#262641",
     "editorMarkerNavigation.background": "#060621",
     "editorMarkerNavigationError.background": "#AB395B",
     "editorMarkerNavigationWarning.background": "#5B7E7A"
   }
 }
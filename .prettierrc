{
  "printWidth": 100,
  "tabWidth": 3,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.{js,jsx,ts,tsx}",
      "options": {
        "jsxWrapAttributes": "auto",
        "jsxWrapAttributesIndentSize": 2,
        "jsxAttributeIndentation": 1
      }
    }
  ]
}
